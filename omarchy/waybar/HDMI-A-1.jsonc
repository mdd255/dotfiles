{
  "layer": "top",
  "position": "top",
  "output": [
    "HDMI-A-1"
  ],
  "modules-center": [
    "custom/earth",
    "clock",
    "clock#us",
    "clock#ptg"
  ],
  "include": [
    "~/.config/dotfiles/omarchy/waybar/base-bar.jsonc"
  ],
  "clock#us": {
    "format": "{:L%a %H:%M %d/%m [Florida]}   ",
    "timezone": "America/New_York"
  },
  "clock#ptg": {
    "format": "{:L%a %H:%M %d/%m [Portugal]}   ",
    "timezone": "Portugal"
  }
}

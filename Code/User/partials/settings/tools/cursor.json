{
	"$schema": "vscode://schemas/settings/default",
	"cursor.chat.alwaysSearchWeb": false,
	"cursor.chat.showSuggestedQuestions": true,
	"cursor.chat.maxCompletionTokens": 4000,
	"cursor.chat.contextLength": 8000,
	"cursor.cpp.enablePartialAccepts": true,
	"cursor.cpp.partialAcceptThreshold": 0.5,
	"cursor.cpp.disabledLanguages": [],
	"cursor.cpp.maxPrefixLengthForSingleLine": 100,
	"cursor.cpp.maxSuffixLengthForSingleLine": 100,
	"cursor.cpp.enablePartialAcceptsInSnippetMode": true,
	"cursor.general.enableModelSelection": true,
	"cursor.general.defaultModel": "claude-4-sonet",
	"cursor.general.fastModel": "gpt-4o-mini",
	"cursor.privacy.anonymizedTelemetry": true,
	"cursor.privacy.enableTelemetry": false,
	"cursor.general.enableIndexing": true,
	"cursor.general.enableDocs": true,
	"cursor.general.enableShadowWorkspace": true,
	"cursor.general.enableAutoSaveBeforeChat": true,
	"cursor.general.enableCodeLens": true,
	"cursor.chat.showChatWelcomeMessage": false,
	"cursor.general.enableInlineChat": true,
	"cursor.general.enableComposer": true,
	"cursor.general.showComposerInSidebar": true,
	"cursor.general.enableDiff": true,
	"cursor.general.enableSyntaxHighlighting": true,
	"cursor.general.enableAutoApply": false,
	"cursor.general.enableSmartSelection": true,
	"cursor.general.enableCursorless": false,
	"cursor.general.enableMultipleProviders": true,
	"cursor.general.enableCustomInstructions": true,
	"cursor.general.enableWorkspaceContext": true,
	"cursor.general.enableGitContext": true,
	"cursor.general.enableFileContext": true,
	"cursor.general.maxFileContextLength": 20000,
	"cursor.general.requestTimeout": 120000,
	"cursor.general.disableTimeouts": true,
	"cursor.general.maxRequestDuration": 300000,
	"cursor.cpp.requestTimeout": 120000
}

{
	"$schema": "vscode://schemas/settings/default",
	"whichkey.bindings": [
		{
			"key": "g",
			"name": " Git",
			"type": "bindings",
			"bindings": [
				{
					"key": "b",
					"name": "󰘬 Branch",
					"type": "bindings",
					"bindings": [
						{
							"key": "c",
							"name": "󱓍 Checkout",
							"type": "command",
							"command": "git.checkout"
						},
						{
							"key": "n",
							"name": "󱓊 New",
							"type": "command",
							"command": "git.branch"
						},
						{
							"key": "d",
							"name": "󱓋 Delete",
							"type": "command",
							"command": "git.deleteBranch"
						},
						{
							"key": "r",
							"name": "󱓎 Rename",
							"type": "command",
							"command": "git.renameBranch"
						}
					]
				},
				{
					"key": "r",
					"name": "󰘬 Reset/Revert",
					"type": "bindings",
					"bindings": [
						{
							"key": "s",
							"name": "󱓍 Reset soft",
							"type": "command",
							"command": "gitlens.gitCommands.reset"
						},
						{
							"key": "h",
							"name": "󱓍 Reset hard",
							"type": "command",
							"command": "git.cleanAll"
						},
						{
							"key": "v",
							"name": "󱓊 Revert",
							"type": "command",
							"command": "gitlens.gitCommands.revert"
						}
					]
				},
				{
					"key": "p",
					"name": "󱓊 Pull/Push",
					"type": "bindings",
					"bindings": [
						{
							"key": "u",
							"name": "󱓍 Push",
							"type": "command",
							"command": "git.push"
						},
						{
							"key": "l",
							"name": "󱓍 Pull",
							"type": "command",
							"command": "git.pull"
						}
					]
				},
				{
					"key": "a",
					"name": "󱓊 Stage/Unstage",
					"type": "bindings",
					"bindings": [
						{
							"key": "f",
							"name": "󱓍 +Current file",
							"type": "command",
							"command": "git.stage"
						},
						{
							"key": "a",
							"name": "󱓍 +All files",
							"type": "command",
							"command": "git.stageAll"
						},
						{
							"key": "d",
							"name": "󱓊 -Current file",
							"type": "command",
							"command": "git.unstage"
						},
						{
							"key": "q",
							"name": "󱓊 -All files",
							"type": "command",
							"command": "git.unstageAll"
						}
					]
				},
				{
					"key": "c",
					"name": "󱓊 Commit/Cherry pick",
					"type": "bindings",
					"bindings": [
						{
							"key": "a",
							"name": "󱓍 Amend all",
							"type": "command",
							"command": "git.commitAllAmend"
						},
						{
							"key": "o",
							"name": "󱓍 Commit all",
							"type": "command",
							"command": "git.commitAll"
						},
						{
							"key": "A",
							"name": "󱓍 Amend",
							"type": "command",
							"command": "git.commitAmend"
						},
						{
							"key": "O",
							"name": "󱓍 Commit",
							"type": "command",
							"command": "git.commit"
						},
						{
							"key": "p",
							"name": "󱓍 Cherrypick",
							"type": "command",
							"command": "git.cherryPick"
						},
						{
							"key": "l",
							"name": "󱓍 Commits",
							"type": "command",
							"command": "gitlens.showCommitSearch"
						}
					]
				},
				{
					"key": "s",
					"name": "󱓊 Status/Stash",
					"type": "bindings",
					"bindings": [
						{
							"key": "t",
							"name": "󱓊 Status",
							"type": "commands",
							"commands": [
								"workbench.action.closeAllEditors",
								"git.openAllChanges"
							]
						},
						{
							"key": "s",
							"name": "󱓎 Stash",
							"type": "command",
							"command": "git.stash"
						},
						{
							"key": "l",
							"name": "󱓎 Stash peek",
							"type": "command",
							"command": "git.stashView"
						},
						{
							"key": "p",
							"name": "󱓎 Stash pop",
							"type": "command",
							"command": "git.stashPopLatest"
						},
						{
							"key": "d",
							"name": "󱓎 Stash drop",
							"type": "command",
							"command": "git.stashDropAll"
						}
					]
				},
				{
					"key": "d",
					"name": " Diff",
					"type": "bindings",
					"bindings": [
						{
							"key": "f",
							"name": " File",
							"type": "command",
							"command": "git.openChange"
						},
						{
							"key": "i",
							"name": " All",
							"type": "command",
							"command": "git.viewChanges"
						}
					]
				},
				{
					"key": "x",
					"name": " Conflict",
					"type": "bindings",
					"bindings": [
						{
							"key": "b",
							"name": " Accept both",
							"type": "command",
							"command": "merge-conflict.accept.both"
						},
						{
							"key": "c",
							"name": " Accept current",
							"type": "command",
							"command": "merge-conflict.accept.current"
						},
						{
							"key": "i",
							"name": " Accept incoming",
							"type": "command",
							"command": "merge-conflict.accept.incoming"
						},
						{
							"key": "B",
							"name": "Accept all both",
							"type": "command",
							"command": "merge-conflict.accept.all-both"
						},
						{
							"key": "C",
							"name": " Accept all current",
							"type": "command",
							"command": "merge-conflict.accept.all-current"
						},
						{
							"key": "I",
							"name": " Accept all incoming",
							"type": "command",
							"command": "merge-conflict.accept.all-incoming"
						},
						{
							"key": "enter",
							"name": " Compare current conflict",
							"type": "command",
							"command": "merge-conflict.compare"
						},
						{
							"key": "n",
							"name": " Next Conflict",
							"type": "command",
							"command": "merge-conflict.next"
						},
						{
							"key": "e",
							"name": " Previous Conflict",
							"type": "command",
							"command": "merge-conflict.previous"
						}
					]
				},
				{
					"key": "i",
					"name": " Init",
					"type": "command",
					"command": "git.init"
				},
				{
					"key": "m",
					"name": " Merge",
					"type": "command",
					"command": "git.merge"
				},
				{
					"key": "g",
					"name": " Goto top",
					"type": "command",
					"command": "cursorTop"
				},
				{
					"key": "n",
					"name": " Next change",
					"type": "command",
					"command": "workbench.action.editor.nextChange"
				},
				{
					"key": "e",
					"name": " Prev change",
					"type": "command",
					"command": "workbench.action.editor.previousChange"
				},
				{
					"key": "d",
					"name": "󱓍 Revert",
					"type": "command",
					"command": "git.revertSelectedRanges"
				},
				{
					"key": "q",
					"name": "󱓎 Clean",
					"type": "command",
					"command": "git.clean"
				},
				{
					"key": "'",
					"name": " Go to bracket",
					"type": "command",
					"command": "editor.action.jumpToBracket"
				}
			]
		}
	]
}

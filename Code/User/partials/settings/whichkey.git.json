{
	"$schema": "vscode://schemas/settings/default",
	"whichkey.bindings": [
		{
			"key": "g",
			"name": "Git",
			"type": "bindings",
			"bindings": [
				{
					"key": "b",
					"name": "*Branch",
					"type": "bindings",
					"bindings": [
						{
							"key": "c",
							"name": "Checkout",
							"type": "command",
							"command": "git.checkout"
						},
						{
							"key": "n",
							"name": "New",
							"type": "command",
							"command": "git.branch"
						},
						{
							"key": "d",
							"name": "Delete",
							"type": "command",
							"command": "git.deleteBranch"
						},
						{
							"key": "r",
							"name": "Rename",
							"type": "command",
							"command": "git.renameBranch"
						}
					]
				},
				{
					"key": "f",
					"name": "*Files",
					"type": "bindings",
					"bindings": [
						{
							"key": "u",
							"name": "Unstage",
							"type": "command",
							"command": "git.unstage"
						},
						{
							"key": "S",
							"name": "Stage all",
							"type": "commands",
							"commands": ["git.stageAll", "workbench.view.scm"]
						},
						{
							"key": "s",
							"name": "Stage",
							"type": "command",
							"command": "git.stage"
						},
						{
							"key": "o",
							"name": "Open",
							"type": "command",
							"command": "gitlens.openOnlyChangedFiles"
						},
						{
							"key": "d",
							"name": "Diff",
							"type": "command",
							"command": "git.viewChanges"
						}
					]
				},
				{
					"key": "c",
					"name": "*Changes",
					"type": "transient",
					"bindings": [
						{
							"key": "q",
							"name": "Revert",
							"type": "command",
							"command": "git.revertSelectedRanges"
						},
						{
							"key": "Q",
							"name": "Clean",
							"type": "command",
							"command": "git.clean"
						},
						{
							"key": "n",
							"name": "Next",
							"type": "command",
							"command": "workbench.action.editor.nextChange"
						},
						{
							"key": "e",
							"name": "Prev",
							"type": "command",
							"command": "workbench.action.editor.previousChange"
						},
						{
							"key": "c",
							"name": "Diff",
							"type": "command",
							"command": "git.openChange"
						}
					]
				},
				{
					"key": "m",
					"name": "*Conflict",
					"type": "transient",
					"bindings": [
						{
							"key": "b",
							"name": "Accept both",
							"type": "command",
							"command": "merge-conflict.accept.both"
						},
						{
							"key": "c",
							"name": "Accept current",
							"type": "command",
							"command": "merge-conflict.accept.current"
						},
						{
							"key": "i",
							"name": "Accept incoming",
							"type": "command",
							"command": "merge-conflict.accept.incoming"
						},
						{
							"key": "B",
							"name": "Accept all both",
							"type": "command",
							"command": "merge-conflict.accept.all-both"
						},
						{
							"key": "C",
							"name": "Accept all current",
							"type": "command",
							"command": "merge-conflict.accept.all-current"
						},
						{
							"key": "I",
							"name": "Accept all incoming",
							"type": "command",
							"command": "merge-conflict.accept.all-incoming"
						},
						{
							"key": "enter",
							"name": "Compare current conflict",
							"type": "command",
							"command": "merge-conflict.compare"
						},
						{
							"key": "n",
							"name": "Next Conflict",
							"type": "command",
							"command": "merge-conflict.next"
						},
						{
							"key": "e",
							"name": "Previous Conflict",
							"type": "command",
							"command": "merge-conflict.previous"
						}
					]
				},
				{
					"key": "o",
					"name": "Commit",
					"type": "command",
					"command": "git.commit"
				},
				{
					"key": "i",
					"name": "Init",
					"type": "command",
					"command": "git.init"
				},
				{
					"key": "x",
					"name": "Merge",
					"type": "command",
					"command": "git.merge"
				},
				{
					"key": "l",
					"name": "Pull",
					"type": "command",
					"command": "git.pull"
				},
				{
					"key": "p",
					"name": "Push",
					"type": "command",
					"command": "git.push"
				},
				{
					"key": "s",
					"name": "Status",
					"type": "command",
					"command": "workbench.view.scm"
				},
				{
					"key": "g",
					"name": "Goto top",
					"type": "command",
					"command": "cursorTop"
				},
				{
					"key": "n",
					"name": "Next change",
					"type": "command",
					"command": "workbench.action.editor.nextChange"
				},
				{
					"key": "e",
					"name": "Prev change",
					"type": "command",
					"command": "workbench.action.editor.previousChange"
				},
				{
					"key": "'",
					"name": "Go to bracket",
					"type": "command",
					"command": "editor.action.jumpToBracket"
				}
			]
		}
	]
}

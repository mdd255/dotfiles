{
	"$schema": "vscode://schemas/settings/default",
	"whichkey.bindings": [
		{
			"key": "f",
			"name": "Find",
			"type": "bindings",
			"bindings": [
				{
					"key": "f",
					"name": "Files",
					"type": "command",
					"command": "workbench.action.quickOpen"
				},
				{
					"key": "s",
					"name": "Search",
					"type": "command",
					"command": "workbench.view.search"
				},
				{
					"key": "o",
					"name": "Symbols",
					"type": "command",
					"command": "workbench.action.quickOpen",
					"args": "@:"
				},
				{
					"key": "O",
					"name": "Workspace symbols",
					"type": "command",
					"command": "workbench.action.quickOpen",
					"args": "#"
				},
				{
					"key": "p",
					"name": "Projects",
					"type": "command",
					"command": "workbench.action.openRecent"
				},
				{
					"key": " ",
					"name": "Find any",
					"type": "command",
					"command": "runCommands",
					"args": {
						"commands": [
							{ "command": "type", "args": { "text": " " } },
							{ "command": "type", "args": { "text": " " } },
							{ "command": "type", "args": { "text": "/" } }
						]
					}
				},
				{
					"key": "l",
					"name": "Goto line",
					"type": "command",
					"command": "runCommands",
					"args": {
						"commands": [
							{ "command": "type", "args": { "text": " " } },
							{ "command": "type", "args": { "text": " " } },
							{ "command": "type", "args": { "text": " " } },
							{ "command": "type", "args": { "text": "b" } },
							{ "command": "type", "args": { "text": "d" } },
							{ "command": "type", "args": { "text": "j" } },
							{ "command": "type", "args": { "text": "k" } }
						]
					}
				}
			]
		}
	]
}

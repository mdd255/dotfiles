{
	"$schema": "vscode://schemas/settings/default",
	"workbench.tips.enabled": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.layoutControl.enabled": false,
	"workbench.editor.mouseBackForwardToNavigate": false,
	"workbench.editor.preventPinnedEditorClose": "keyboard",
	"workbench.editor.untitled.labelFormat": "name",
	"workbench.editor.labelFormat": "short",
	"workbench.editor.enablePreview": false,
	"workbench.activityBar.location": "hidden",
	"workbench.editor.showTabs": "multiple",
	"workbench.layoutControl.type": "menu",
	"workbench.reduceMotion": "on",
	"workbench.editor.tabActionCloseVisibility": false,
	"workbench.editor.tabActionUnpinVisibility": false,
	"workbench.editor.splitOnDragAndDrop": false,
	"workbench.editor.decorations.colors": false,
	"workbench.editor.decorations.badges": false,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"workbench.tree.indent": 15,
	"workbench.editor.closeOnFileDelete": true,
	"workbench.editor.limit.enabled": true,
	"workbench.editor.limit.value": 10
}

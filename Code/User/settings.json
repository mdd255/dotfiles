{
	"$schema": "vscode://schemas/settings/default",
	"editor.fastScrollSensitivity": 5,
	"editor.smoothScrolling": true,
	"editor.inlineSuggest.enabled": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.inlayHints.enabled": "off",
	"editor.hover.enabled": false,
	"editor.formatOnPaste": true,
	"editor.tabSize": 2,
	"editor.fontSize": 14,
	"editor.renderWhitespace": "trailing",
	"editor.renderLineHighlight": "line",
	"editor.lineHeight": 1.2,
	"editor.codeActionsOnSave": { },
	"editor.fontFamily": "FiraCode Nerd Font",
	"editor.fontLigatures": true,
	"editor.insertSpaces": false,
	"editor.formatOnSave": true,
	"editor.lineNumbers": "relative",
	"editor.inlineSuggest.suppressSuggestions": false,
	"editor.minimap.enabled": false,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.cursorBlinking": "smooth",
	"editor.stickyScroll.enabled": false,
	"editor.matchBrackets": "always",
	"editor.guides.bracketPairs": "active",
	"editor.scrollbar.horizontal": "hidden",
	"editor.scrollbar.verticalScrollbarSize": 5,
	"editor.overviewRulerBorder": false,
	"editor.glyphMargin": false,
	"editor.showFoldingControls": "never",
	"editor.lightbulb.enabled": "off",
	"editor.experimental.treeSitterTelemetry": false,
	"editor.formatOnSaveMode": "modificationsIfAvailable",
	"editor.inlineSuggest.syntaxHighlightingEnabled": true,
	"editor.codeLens": false,
	"editor.suggest.preview": true,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.acceptSuggestionOnEnter": "smart",
	"editor.snippetSuggestions": "inline",
	"editor.suggestOnTriggerCharacters": true,
	"editor.wordBasedSuggestions": "off",
	"editor.parameterHints.enabled": false,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"editor.accessibilitySupport": "off",
	"editor.find.autoFindInSelection": "multiline",
	"editor.gotoLocation.multipleReferences": "goto",
	"editor.gotoLocation.multipleDefinitions": "goto",
	"editor.hover.delay": 300,
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairsHorizontal": true,
	"editor.quickSuggestions": {
		"other": "on",
		"comments": "off",
		"strings": "off"
	},
	"explorer.openEditors.visible": 1,
	"explorer.decorations.badges": false,
	"explorer.decorations.colors": true,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.compactFolders": false,
	"files.enableTrash": false,
	"files.confirmDelete": false,
	"files.trimTrailingWhitespace": true,
	"files.insertFinalNewline": true,
	"extensions.experimental.deferredStartupFinishedActivation": true,
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1,
		"vspacecode.whichkey": 2,
		"alefragnani.bookmarks": 3,
		"be5invis.vscode-custom-css": 3,
		"biomejs.biome": 3,
		"bmewburn.vscode-intelephense-client": 3,
		"brandonkirbyson.vscode-animations": 3,
		"christian-kohler.path-intellisense": 3,
		"compulim.vscode-clock": 3,
		"cweijan.dbclient-jdbc": 3,
		"cweijan.vscode-database-client2": 3,
		"cyansprite.smoothscroll": 3,
		"eamodio.gitlens": 3,
		"github.copilot": 3,
		"github.copilot-chat": 3,
		"github.vscode-github-actions": 3,
		"github.vscode-pull-request-github": 3,
		"golang.go": 3,
		"graphql.vscode-graphql": 3,
		"graphql.vscode-graphql-execution": 3,
		"graphql.vscode-graphql-syntax": 3,
		"humao.rest-client": 3,
		"mikestead.dotenv": 3,
		"ms-python.python": 3,
		"ms-python.vscode-pylance": 3,
		"ms-python.vscode-python-envs": 3,
		"ms-vscode-remote.remote-containers": 3,
		"naumovs.color-highlight": 3,
		"oderwat.indent-rainbow": 3,
		"prisma.prisma": 3,
		"redhat.java": 3,
		"rexshi.phpdoc-comment-vscode-plugin": 3,
		"streetsidesoftware.avro": 3,
		"streetsidesoftware.code-spell-checker": 3,
		"sumneko.lua": 3,
		"usernamehw.errorlens": 3,
		"yoavbls.pretty-ts-errors": 3
	},
	"extensions.ignoreRecommendations": true,
	"extensions.autoCheckUpdates": true,
	"extensions.autoUpdate": true,
	"files.hotExit": "onExitAndWindowClose",
	"files.simpleDialog.enable": true,
	"files.exclude": {
		"**/dist": true,
		"**/.next": true,
		"**/assets": true,
		"**/node_modules": true,
		"**/logs": true,
		"yarn.lock": true,
		"package-lock.json": true,
		"pnpm-lock.yaml": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/tmp/**": true,
		"**/dist/**": true,
		"**/.next/**": true,
		"**/target/**": true,
		"**/.gradle/**": true,
		"**/build/**": true
	},
	"keyboard.dispatch": "keyCode",
	"telemetry.telemetryLevel": "off",
	"update.showReleaseNotes": false,
	"notebook.gotoSymbols.showAllSymbols": false,
	"breadcrumbs.enabled": true,
	"breadcrumbs.symbolSortOrder": "type",
	"outline.icons": false,
	"makefile.configureOnOpen": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"workbench.colorTheme": "Default Dark Modern",
	"search.smartCase": true,
	"search.showLineNumbers": false,
	"search.decorations.badges": false,
	"search.decorations.colors": false,
	"search.collapseResults": "alwaysCollapse",
	"search.useIgnoreFiles": false,
	"search.searchOnType": true,
	"search.searchOnTypeDebounceTime": 500,
	"search.followSymlinks": false,
	"search.maxResults": 1000,
	"search.searchEditor.defaultNumberOfContextLines": 1,
	"search.searchEditor.reusePriorSearchConfiguration": true,
	"search.searchEditor.showLineNumbers": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/coverage": true,
		"**/dist": true,
		"**/build": true,
		"**/.git": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/*.min.js": true,
		"**/*.min.css": true,
		"**/logs": true,
		"**/tmp": true,
		"**/temp": true,
		"**/.cache": true,
		"**/CachedData": true,
		"**/Cache": true,
		"**/History": true,
		"**/SharedStorage": true,
		"**/blob_storage": true,
		"**/*.lock": true
	},
	"security.workspace.trust.untrustedFiles": "open",
	"security.workspace.trust.startupPrompt": "never",
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.banner": "never",
	"terminal.integrated.copyOnSelection": true,
	"terminal.integrated.cursorBlinking": true,
	"terminal.explorerKind": "external",
	"terminal.external.linuxExec": "wezterm",
	"terminal.integrated.tabs.enabled": false,
	"terminal.integrated.showExitAlert": false,
	"terminal.integrated.smoothScrolling": true,
	"terminal.integrated.gpuAcceleration": "on",
	"terminal.integrated.defaultLocation": "editor",
	"terminal.integrated.tabs.title": "${workspaceFolderName}",
	"material-icon-theme.hidesExplorerArrows": true,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorCustomizations": {
		"[Default Dark Modern]": {
			"editor.background": "#0c0c0c",
			"tab.activeBackground": "#0c0c0c",
			"terminal.background": "#0c0c0c",
			"quickInput.background": "#0c0c0c",
			"menu.background": "#0c0c0c",
			"sideBar.background": "#0c0c0c",
			"statusBar.background": "#0c0c0c",
			"panel.background": "#0c0c0c",
			"input.background": "#0c0c0c",
			"editorError.background": "#0c0c0c00",
			"editorWarning.background": "#0c0c0c00",
			"editorInfo.background": "#0c0c0c00",
			"editor.lineHighlightBackground": "#1d1d1d"
		}
	},
	"window.density.editorTabHeight": "compact",
	"window.title": "${folderName}",
	"window.customTitleBarVisibility": "auto",
	"window.menuBarVisibility": "hidden",
	"window.commandCenter": false,
	"window.titleBarStyle": "native",
	"window.zoomLevel": 1,
	"window.confirmSaveUntitledWorkspace": false,
	"workbench.tips.enabled": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.layoutControl.enabled": false,
	"workbench.editor.mouseBackForwardToNavigate": false,
	"workbench.editor.preventPinnedEditorClose": "keyboard",
	"workbench.editor.untitled.labelFormat": "name",
	"workbench.editor.labelFormat": "short",
	"workbench.editor.enablePreview": false,
	"workbench.activityBar.location": "hidden",
	"workbench.editor.showTabs": "multiple",
	"workbench.layoutControl.type": "menu",
	"workbench.reduceMotion": "on",
	"workbench.editor.tabActionCloseVisibility": false,
	"workbench.editor.tabActionUnpinVisibility": false,
	"workbench.editor.splitOnDragAndDrop": false,
	"workbench.editor.decorations.colors": false,
	"workbench.editor.decorations.badges": false,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"workbench.editor.closeOnFileDelete": true,
	"workbench.editor.limit.enabled": true,
	"workbench.activityBar.orientation": "vertical",
	"workbench.tree.enableStickyScroll": false,
	"workbench.tree.renderIndentGuides": "always",
	"workbench.tree.indent": 18,
	"workbench.editor.limit.value": 10,
	"animations.Install-Method": "Custom CSS and JS",
	"animations.Enabled": true,
	"animations.Scrolling": "Fade",
	"animations.Tabs": "Slide",
	"animations.CursorAnimationOptions": {
		"Color": "#569cd6",
		"CursorStyle": "block",
		"TrailLength": 6
	},
	"animations.CursorAnimation": true,
	"vscode_custom_css.imports": [
		"file://${userHome}/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.7/dist/updateHandler.js",
		"file://${userHome}/.config/dotfiles/Code/User/settings.css"
	],
	"biome.enabled": true,
	"biome.rename": true,
	"clock.dateFormat": "HH:MM dd/mm",
	"color-highlight.markerType": "underline",
	"color-highlight.markRuler": false,
	"github.copilot.chat.agent.thinkingTool": true,
	"github.copilot.chat.codesearch.enabled": true,
	"github.copilot.advanced": {
		"useLanguageServer": true
	},
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false
	},
	"github.copilot.editor.enableCodeActions": false,
	"github.copilot.enableAutoCompletions": true,
	"database-client.highlightSQLBlock": true,
	"database-client.capitalizeKeywordsWhenFormatting": true,
	"database-client.queryWithSchema": true,
	"database-client.enableEditorCodeLens": false,
	"database-client.autoSync": true,
	"errorLens.messageBackgroundMode": "none",
	"errorLens.messageTemplate": "$message [$source:$code]",
	"indentRainbow.indicatorStyle": "light",
	"indentRainbow.colorOnWhiteSpaceOnly": true,
	"indentRainbow.lightIndicatorStyleLineWidth": 1,
	"indentRainbow.colors": [
		"rgba(255,255,64,0.07)",
		"rgba(127,255,127,0.07)",
		"rgba(255,127,255,0.07)",
		"rgba(79,236,236,0.07)"
	],
	"vscode-neovim.ctrlKeysForInsertMode": [ ],
	"vscode-neovim.ctrlKeysForNormalMode": [ ],
	"vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
	"outline.showArrays": false,
	"outline.showBooleans": false,
	"outline.showConstants": false,
	"outline.showConstructors": false,
	"outline.showEnumMembers": false,
	"outline.showEnums": false,
	"outline.showEvents": false,
	"outline.showFields": false,
	"outline.showFiles": false,
	"outline.showKeys": false,
	"outline.showNull": false,
	"outline.showNumbers": false,
	"outline.showObjects": false,
	"outline.showOperators": false,
	"outline.showPackages": false,
	"outline.showStrings": false,
	"outline.showVariables": false,
	"outline.showTypeParameters": false,
	"outline.showProperties": false,
	"redhat.telemetry.enabled": false,
	"rest-client.requestNameAsResponseTabTitle": true,
	"rest-client.disableHighlightResonseBodyForLargeResponse": true,
	"rest-client.enableTelemetry": false,
	"rest-client.enableSendRequestCodeLens": false,
	"rest-client.previewResponseInUntitledDocument": true,
	"rest-client.showResponseInDifferentTab": false,
	"whichkey.delay": 0,
	"whichkey.showIcons": true,
	"whichkey.bindings": [
		{
			"key": "s",
			"name": " Editors",
			"type": "bindings",
			"bindings": [
				{
					"key": "n",
					"name": " Split down",
					"type": "command",
					"command": "workbench.action.splitEditorDown"
				},
				{
					"key": "e",
					"name": " Vsplit right",
					"type": "command",
					"command": "workbench.action.splitEditorRight"
				},
				{
					"key": "y",
					"name": " Copy current file path",
					"type": "commands",
					"commands": [
						"copyRelativeFilePath",
						"vscode-neovim.send"
					],
					"args": [
						null,
						":lua vim.notify('Copied: ' .. vim.fn.getreg('+'))<CR>"
					]
				},
				{
					"key": "N",
					"name": " New",
					"type": "command",
					"command": "workbench.action.files.newUntitledFile"
				},
				{
					"key": "r",
					"name": " Restore",
					"type": "command",
					"command": "workbench.action.reopenClosedEditor"
				},
				{
					"key": "s",
					"name": " Save",
					"type": "command",
					"command": "workbench.action.files.save"
				}
			]
		},
		{
			"key": "f",
			"name": " Find",
			"type": "bindings",
			"bindings": [
				{
					"key": "f",
					"name": " Files",
					"type": "command",
					"command": "workbench.action.quickOpen"
				},
				{
					"key": "l",
					"name": "󰞷 Goto line",
					"type": "key",
					"command": "vscode-neovim.send",
					"args": ":"
				},
				{
					"key": "s",
					"name": " Search",
					"type": "command",
					"command": "search.action.openEditor"
				},
				{
					"key": "r",
					"name": " Search & replace",
					"type": "command",
					"command": "workbench.view.search"
				},
				{
					"key": "o",
					"name": " Symbols",
					"type": "command",
